name: basic_cleaning
conda_env: conda.yml

entry_points:
  main:
    parameters:

      input_artifact:
        description: Full name of the input artifact (source data)
        type: string

      output_artifact:
        description: Full name of the output artifact
        type: string


    command: >-
        python run.py  --parameter1 {parameter1}  --parameter2 {parameter2}
